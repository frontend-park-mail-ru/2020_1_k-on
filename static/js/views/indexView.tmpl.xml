<fest:template xmlns:fest="http://fest.mail.ru" content-name="data">
    <p class="index-page__headline">Сериалы</p>

    <div class="series-block">
        <fest:for iterate="data" index="i" value="v">
            <a class="series-card">
                <div class="series-card__image" style="background-image: v.image">
                    <span class="series-card__age-limit">v.ageLimit+</span>
                </div>
                <span class="series-card__name">v.name</span>
            </a>
        </fest:for>
    </div>
</fest:template>
